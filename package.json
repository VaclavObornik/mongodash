{
  "name": "mongodash-xxx-test",
  "version": "0.0.2",
  "description": "An utility library delivering super-useful and super-simple tools using MongoDB",
  "main": "dist/mongodash.umd.js",
  "module": "dist/mongodash.es5.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist docs",
    "build": "tsc && rollup --config ./rollup.config.ts && npm run docs",
    "docs": "typedoc --theme ./node_modules/@bndynet/typedoc-default-themes/bin/default --out docs/api --mode file",
    "watch": "rollup --config ./rollup.config.ts -w",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run site-serv\"",
    "site-serv": "run-p docs test --aggregate-output && lite-server",

    "test": "run-p test:lint test:coverage test:stryker --aggregate-output",
    "test:lint": "eslint ./src/**/*.ts",
    "test:coverage": "jest --coverage",
    "test:coverage:noCache": "npm run test:coverage -- --no-cache",
    "test:watch": "jest --coverage --watch",
    "test:prod": "run-p test:lint test:coverage:noCache test:stryker --aggregate-output",
    "test:stryker": "stryker run",

    "predeploy-gh-pages": "ts-node tools/gh-pages-prepare",
    "deploy-gh-pages": "npm run predeploy-gh-pages && ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "republish": "ts-node tools/prepare-republish.ts",

    "init": "ts-node tools/init.ts"
  },
  "keywords": [
    "mongodb",
    "utility",
    "library"
  ],
  "author": {
    "name": "Václav Oborník",
    "email": "vaclav.obornik@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VaclavObornik/mongodash.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@bndynet/cli": "^1.3.0",
    "@bndynet/typedoc-default-themes": "^0.5.1",
    "@commitlint/cli": "^7.6.1",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@stryker-mutator/core": "^4.4.1",
    "@stryker-mutator/jest-runner": "^4.4.1",
    "@types/jest": "^23.3.14",
    "@types/node": "^12.19.15",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^9.8.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "jest-config": "^23.6.0",
    "jest-transform-stub": "^1.0.0",
    "jsdom": "13.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^8.2.1",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "organize-imports-cli": "^0.8.0",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "rollup": "^2.38.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^17.3.7",
    "ts-jest": "^24.3.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "organize-imports-cli",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {}
}
